@model IEnumerable<CxC_Seminario.DO.DistritoEclesiastico>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<p>
    <div class="container-fluid">
        <a href="@Url.Action("Create", "DistritoEclesiastico")" class="btn btn-primary btn-lg"> Agregar Nuevo</a>
    </div>
</p>

<div class="sparkline8-list">
    <div class="sparkline8-graph">
        <div class="static-table-list">
                <table class="table">
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.Nombre)
                        </th>
                        <th></th>
                    </tr>

                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.Nombre)
                            </td>
                            <td>
                                @Html.ActionLink("Editar", "Edit", new { id = item.IdDistritoEclesiastico },new { @class = "btn btn-warning btn-sm" }) |
                                @Html.ActionLink("Eliminar", "Delete", new { id = item.IdDistritoEclesiastico }, new { @class = "btn btn-danger btn-sm" })
                            </td>
                        </tr>
                    }

                </table>
        </div>
    </div>
</div>